{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (API - HTTP)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/MyFinanceTracker.Api/bin/Debug/net10.0/MyFinanceTracker.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/MyFinanceTracker.Api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_LAUNCH_PROFILE": "http"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "launchBrowser": {
        "enabled": false
      },
      "console": "internalConsole"
    },
    {
      "name": ".NET Core Launch (API - HTTPS)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/MyFinanceTracker.Api/bin/Debug/net10.0/MyFinanceTracker.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/MyFinanceTracker.Api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_LAUNCH_PROFILE": "https"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      },
      "launchBrowser": {
        "enabled": false
      },
      "console": "internalConsole"
    }
  ]
}
